<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tarifkupu"
        tools:targetApi="31">
        <activity
            android:name="com.beraterdem2.tarifkupu2.animasion.AnimationActivity"
            android:exported="true"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.beraterdem2.tarifkupu2.view.BranceDetailsActivity"
            android:exported="false"
            android:parentActivityName="com.beraterdem2.tarifkupu2.view2.BranceActivity"/>
        <activity
            android:name="com.beraterdem2.tarifkupu2.view2.BranceActivity"
            android:exported="false"
            android:parentActivityName="com.beraterdem2.tarifkupu2.view.MainActivity"/>
        <activity
            android:name="com.beraterdem2.tarifkupu2.view2.AddBranceActivity"
            android:exported="false"
            android:parentActivityName="com.beraterdem2.tarifkupu2.view.MainActivity"/>
        <activity
            android:name="com.beraterdem2.tarifkupu2.view.DetailsActivity"
            android:exported="false"
            android:parentActivityName="com.beraterdem2.tarifkupu2.view.MainActivity"/>
        <activity
            android:name="com.beraterdem2.tarifkupu2.view.MainActivity"
            android:exported="false">

        </activity>


    </application>

</manifest>